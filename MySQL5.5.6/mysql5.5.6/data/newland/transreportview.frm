query=select `n`.`networkid` AS `networkid`,`n`.`networkcode` AS `networkcode`,`n`.`networkname` AS `networkname`,`r`.`authid` AS `authid`,`r`.`workstationcode` AS `workstationcode`,`r`.`trans_code` AS `trans_code`,`r`.`trans_amount` AS `trans_amount`,`r`.`priority` AS `priority`,`r`.`trans_date` AS `trans_date`,`r`.`trans_time` AS `trans_time`,`r`.`auth_date` AS `auth_date`,`r`.`auth_time` AS `auth_time`,`r`.`waterid` AS `waterid`,`r`.`state` AS `state`,`r`.`managerid` AS `managerid`,`r`.`workerid` AS `workerid`,`r`.`refuse_reason` AS `refuse_reason`,(case when (`r`.`state` = 0) then \'未授权\' else (case when (`r`.`state` = 1) then \'授权处理中\' else (case when (`r`.`state` = 2) then \'已授权\' else (case when (`r`.`state` = 3) then \'授权主管拒绝\' else (case when (`r`.`state` = 4) then \'柜员主动撤销\' else (case when (`r`.`state` = 5) then \'错账\' end) end) end) end) end) end) AS `statename`,`m`.`name` AS `managername`,`m2`.`name` AS `workername`,`t`.`trans_name` AS `trans_name` from ((((`newland`.`t_requestqueque` `r` left join `newland`.`t_manager` `m` on((`r`.`managerid` = `m`.`code`))) left join `newland`.`t_manager` `m2` on((`r`.`workerid` = `m2`.`code`))) left join `newland`.`t_trans_code` `t` on((`r`.`trans_code` = `t`.`trans_code`))) left join `newland`.`viewnetworkstation` `n` on((`r`.`workstationcode` = `n`.`workcode`)))
